  "compilerOptions": {
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "skipLibCheck": true,
    "types": [
      "node"
    ]
  },
  "include": [
    "src/shared/**/*",
    "src/static-build/**/*",
    "src/client/**/*",
    "src/features/**/*",
    "src/worker-shared/**/*",
    "src/features-worker/**/*"
  ]
}